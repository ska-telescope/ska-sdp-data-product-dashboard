  "author": "Andre Odendaal <andre@vivosa.co.za>",
  "license": "BSD 3-Clause",
  "scripts": {
    "build": "webpack --mode production",
    "code-analysis": "./node_modules/.bin/eslint \"src/**\" --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:component": "cypress run --component",
    "dev": "webpack serve --mode development --open",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "c install; yarn-deduplicate --strategy=fewer; yarn install",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "lint:prettier": "prettier --check \"**/*\"",
    "merge-reports": "jrm build/reports/unit-tests.xml \"build/tests/*.xml\"",
    "start": "NODE_ENV=test webpack serve --mode development --open",
    "test:component:headless": "cypress run --component --headless --browser chrome",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports --reporter=cobertura",
    "test:coverage:summary": "npx nyc report --reporter=text-summary",
    "test:cypress:component:ci": "cypress run --component --headless --browser chrome --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml",
    "test:cypress:e2e:ci": "cypress run --e2e --headless --browser chrome --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml",
    "test:e2e:headless": "cypress run --e2e --headless --browser chrome",
    "test:headless": "yarn cypress:run"
  },
