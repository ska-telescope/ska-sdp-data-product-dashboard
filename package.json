  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "code-analysis": "eslint \"src/**\" --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:run:component": "cross-env cypress run --component --config video=false",
    "dev": "npx react-inject-env set -d public/ && webpack serve --mode development --open",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:yarn-deduplicate": "c install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "merge-reports": "jrm build/reports/unit-tests.xml \"build/tests/*.xml\"",
    "prettier": "prettier --check \"**/*\"",
    "prettier:fix": "prettier --write \"**/*\"",
    "skao:update": "yarn add @ska-telescope/ska-gui-components@latest",
    "start": "npx react-inject-env set -d public/ && NODE_ENV=test webpack serve --mode development --open",
    "test:component:headless": "cross-env cypress run --component --headless --browser chrome --config video=false",
    "test:coverage:report": "npx nyc report --report-dir=build/reports --reporter=html",
    "test:coverage:report:ci": "npx nyc report --report-dir=build/reports --reporter=cobertura",
    "test:coverage:summary": "npx nyc report --reporter=text-summary",
    "test:cypress:component:ci": "cross-env cypress run --component --headless --browser chrome --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml --config video=false",
    "test:cypress:e2e:ci": "cypress run --e2e --headless --browser chrome --reporter junit --reporter-options mochaFile=build/tests/unit-tests[hash].xml --config video=false",
    "test:e2e:headless": "cypress run --e2e --headless --browser chrome --config video=false",
    "test:headless": "yarn cypress:run"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
