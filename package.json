{
  "name": "ska-sdp-dataproduct-dashboard",
  "version": "0.2.3",
  "private": true,
  "main": "index.js",
  "repository": "git@gitlab.com:ska-telescope/sdp/ska-sdp-dataproduct-dashboard.git",
  "author": "Andre Odendaal <andre@vivosa.co.za>",
  "license": "BSD 3-Clause",
  "scripts": {
    "build": "webpack --mode production",
    "code-analysis": "./node_modules/.bin/eslint \"src/**\" --ignore-path .eslintignore -f junit -o linting.xml",
    "cypress:component:headless": "cypress run --component --headless",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "nyc react-scripts test --env=jsdom --reporters=default --reporters=jest-junit --watchAll=false",
    "test:coverage": "nyc react-scripts test --env=jsdom --watchAll=false --coverage --coverageDirectory=build/coverage",
    "test:watch": "react-scripts test --env=jsdom --watchAll=true",
    "start": "webpack serve --mode development --open",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "c install; yarn-deduplicate --strategy=fewer; yarn install",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "yarn tsc --noEmit",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "cypress:headless:chrome": "cypress run --component --browser chrome"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/react": "^7.0.2",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.92",
    "@mui/material": "^5.11.8",
    "@mui/x-data-grid": "^5.17.23",
    "@mui/x-date-pickers": "^5.0.17",
    "@rollup/plugin-typescript": "^11.0.0",
    "@ska-telescope/ska-javascript-components": "^0.1.13",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "axios": "^0.27.2",
    "babel-loader": "^9.1.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bundle-loader": "^0.5.6",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.0.1",
    "downloadjs": "^1.4.7",
    "framer-motion": "^6.4.3",
    "i18next": "^21.6.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "moment": "^2.29.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-folder-tree": "^5.0.3",
    "react-i18next": "^11.15.3",
    "webpack": "^5.76.2",
    "webpack-dev-middleware": "^6.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.17.9",
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/webpack-preprocessor": "^5.16.3",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/downloadjs": "^1.4.3",
    "@types/node": "^18.6.1",
    "babel-plugin-import": "^1.13.5",
    "css-loader": "^6.7.1",
    "cypress": "^12.5.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "html-webpack-plugin": "^5.5.0",
    "i": "^0.3.7",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^13.0.0",
    "junit": "^1.4.9",
    "msw": "^0.48.3",
    "nyc": "^15.1.0",
    "or": "^0.2.0",
    "prettier": "2.7.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^16.7.0",
    "rewiremock": "^3.14.4",
    "sass": "^1.49.4",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "testing-library": "^0.0.2",
    "webpack-cli": "^5.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/{!(i18n),}.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/index.jsx"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "html"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
